{
    "manifest_version": 3,
    "name": "Skyward Attendance Scraper",
    "version": "1.0",
    "description": "Easily view and filter skyward attendance history",
    "permissions": ["scripting", "activeTab", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  